{
	"endOfLine": "auto",
	"semi": false,
	"singleQuote": false,
	"printWidth": 85,
	"arrowParens": "always",
	"tabWidth": 2,
	"useTabs": true,
	"trailingComma": "es5",
	"importOrder": [
		"^(react/(.*)$)|^(react$)",
		"",
		"<THIRD_PARTY_MODULES>",
		"",
		"^types$",
		"^@/components/(.*)$",
		"^@/components/ui/(.*)$",
		"^@/constants/(.*)$",
		"^@/context/(.*)$",
		"^@/env/(.*)$",
		"^@/hooks/(.*)$",
		"^@/layout/(.*)$",
		"^@/libs/(.*)$",
		"^@/modules/(.*)$",
		"^@/styles/(.*)$",
		"^@/types/(.*)$",
		"^@/utils/(.*)$",
		"",
		"^[./]"
	],
	"importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
	"importOrderTypeScriptVersion": "5.0.0",
	"plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"]
}
